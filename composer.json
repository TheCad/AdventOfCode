{
    "name": "thecad/advent-of-code",
    "description": "Advent of Code Repository",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Thecad\\AdventOfCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thecad\\AdventOfCode\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit tests/ --color"
    },
    "authors": [
        {
            "name": "TheCad"
        }
