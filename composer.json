    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^6.1",
        "vanilla/garden-cli": "^3.1",
        "phpunit/phpunit": "^9"
    },
    "require": {
        "symfony/dotenv": "^6.1",
        "ext-curl": "*",
        "ext-sqlite3": "*"
    }
}
